@model IEnumerable<WebApplication1.Models.ShoppingCartItemBridge>

@{
    ViewBag.Title = "Index";
}


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Product.productName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ShoppingCarts.cartID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Product.productName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ShoppingCarts.cartID)
        </td>


    </tr>
}

</table>
